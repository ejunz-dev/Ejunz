{
    "name": "ejunz-workspace",
    "private": true,
    "workspaces": [
        "packages/*",
        "framework/*",
        "plugins/*",
        "modules/*"
    ],
    "main": "package.json",
    "scripts": {
        "debug": "node --trace-warnings --async-stack-traces --trace-deprecation packages/ejun/bin/ejun --debug --template",
        "build:watch": "node build/prepare.js && tsc -b --watch",
        "build:ui:dev": "node packages/ui-default/build --iconfont && node --trace-deprecation packages/ui-default/build --dev",
        "build:ui": "node packages/ui-default/build --iconfont && node packages/ui-default/build",
        "build:ui:iconfont": "node packages/ui-default/build --iconfont",
        "build:ui:dev:https": "node packages/ui-default/build --iconfont && node --trace-deprecation packages/ui-default/build --dev --https",
        "build:ui:production": "cross-env NODE_OPTIONS=--max_old_space_size=32000 node packages/ui-default/build --iconfont && node packages/ui-default/build --production",
        "build:ui:production:webpack": "cross-env NODE_OPTIONS=--max_old_space_size=8192 node packages/ui-default/build --production",
        "start": "packages/ejun/bin/ejun.js",
        "ejun": "./node_modules/.bin/ejun"
    },
    "version": "0.0.0",
    "license": "AGPL-3.0-only",
    "devDependencies": {
        "@simplewebauthn/types": "9.0.1",
        "@types/autocannon": "^7.12.5",
        "@types/chai": "^4.3.20",
        "@types/cross-spawn": "^6.0.6",
        "@types/node": "20.12.12",
        "@types/semver": "^7.5.8",
        "autocannon": "^7.15.0",
        "cac": "^6.7.14",
        "chai": "^5.1.1",
        "cross-env": "^7.0.3",
        "cross-spawn": "^7.0.3",
        "esbuild": "0.23.0",
        "eslint": "^8.57.1",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-import-resolver-webpack": "^0.13.9",
        "fs-extra": "^11.2.0",
        "globby": "^14.0.2",
        "inspectpack": "^4.7.1",
        "mongodb-memory-server": "9.4.1",
        "nyc": "^17.1.0",
        "ora": "^8.1.0",
        "package-json": "^10.0.1",
        "semver": "^7.6.3",
        "simple-git": "^3.27.0",
        "supertest": "^7.0.0",
        "typescript": "5.3.3",
        "wtfnode": "^0.9.3"
    },
    "resolutions": {
        "@cordisjs/logger": "0.3.2",
        "reggol": "1.6.3"
    }
}
